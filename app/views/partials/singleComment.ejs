<tr>
<td>
    <table>
    <tbody>
    <tr>
    <td><img style="width: <%= level * 20 %>px" src="https://news.ycombinator.com/s.gif"/></td>
    <td class="votelink">
        <a id="" href="#">
            <img src="https://news.ycombinator.com/grayarrow.gif" class="votearrow <%= contribution.upvoted.toString().includes(userId) ? 'voted' : '' %>" 
            alt="upvote" id="vote_<%= contribution._id %>" 
            onclick="vote('<%= contribution._id %>')"></img>
        </a>
    </td>
    <td class="subtext">
        <span><span id="votes_<%= contribution._id %>"><%= contribution.upvoted.length %></span> Points by <%if(typeof contribution.user === "undefined"){%>
            Undefined user
        <%}else{%>
            <a href="/user?id=<%=contribution.user._id%>"><%=contribution.user.username %></a> 
        <%}%>  
        <a href="item?id=<%= contribution._id %>"><%= contribution.since %> ago</a></span>
        <div class="athing">
        <%= contribution.content %>
        </div>
        <span onclick="unvote('<%= contribution._id %>')" 
                id="unvote_<%= contribution._id %>"
                class="<%= contribution.upvoted.toString().includes(userId) ? '' : 'unvoted' %>" ><a href="#"> unvote</a>| </span> 
        <span><a href="/item?id=<%= contribution._id %>">reply</a></span>
    </td>
    </tr>
    </tbody>
    </table>
</td>
</tr>
