<tr>
    <% if(!typeof counter === 'undefined')  { %>
    <td> <% counter += 1 %>  </td>
    <td class="count">
        <%= counter + "." %>
    </td>
    <% } %>
    <td class="votelink">
        <a id="" href="#">
            <img src="https://news.ycombinator.com/grayarrow.gif" class="votearrow" title="upvote"></img>
        </a>
    </td>
    <td class="athing">
        <% if (contribution.contributionType == 'url') { %>
            <a href="<%= contribution.content %>"><%= contribution.title %></a> | <a href="http://<%= contribution.shortUrl %>"><%= contribution.shortUrl %></a>
        <% } else { %>
            <a href="item?id=<%= contribution._id %>">Ask HN: <%= contribution.title %></a>
        <% } %>
    </td>
</tr>
<tr>
    <% if(!typeof counter === 'undefined')  { %>
    <td></td>
    <td></td>
    <% } %>
    <td></td>
    <td class="subtext">
        <span><%= contribution.upvoted.length %> Points by 
            <%if(typeof contribution.user === "undefined"){%>
                Undefined user
            <%}else{%>
                <a href="/user?id=<%=contribution.user._id%>"><%=contribution.user.username%></a> 
            <%}%> 
            <a href="item?id=<%= contribution._id %>"><%= contribution.since %> ago</a> | <a href="item?id=<%= contribution._id %>"><%= contribution.nComments %> comment/s</a></span>
    </td>
</tr>