<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>
  <header>
    <% include ../partials/nav.ejs %>
  </header>
  <main>
        <table>
            <tbody>
                <% include ../partials/singleContribution.ejs %>
                <% if (contribution.contributionType == 'ask') { %>
                    <tr><td></td><td class="athing"><%= contribution.content %></td></tr>
                <% } %>
                <% contribution.child.forEach((child) => { %>
                    <p><%= child.content %></p>
                <% }); %>
            </tbody>
        </table>
  </main>
  <% include ../partials/footer.ejs %>
</body>
</html>